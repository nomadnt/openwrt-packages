config chilli
	option enabled '0'
	option interval '3600'
	option net '192.168.1.0/24'
	option uamlisten '192.168.1.1'
	option uamport '3990'
	option network 'lan'
	option uamallowed '192.168.1.1,nomadnt.com'
	option uamanydns '1'
	option domain 'lan'
	option dns1 '208.67.222.222'
	option dns2 '208.67.220.220'
	option uamhomepage 'http://192.168.1.1:3990/www/splash.html'
	option wwwdir '/etc/chilli/www'
	option wwwbin '/etc/chilli/wwwsh'
	list uamdomain '.paypal.com'
	list uamdomain '.paypalobjects.com'
	option locationname 'MeOS Hotspot'
	option radiuslocationname 'MeOS_Hotspot'
	option radiuslocationid 'isocc=IT,cc=39,ac=0932,network=Coova,MeOS_Network'
	option radiusserver1 'radius.nomadnt.com' 
	option radiusserver2 'radius.nomadnt.com'
	option radiussecret 'mystrongsecret'
	option radiusauthport '1812'
	option radiusacctport '1813'
	option uamserver 'http://192.168.1.1:4990/www/login.chi'
	option radiusnasid 'NAS01'
	option papalwaysok '1'
	option uamaliasname 'MeOS Hotspot'
	option adminupdatefile '/etc/chilli/local.conf'
	option uamsecret 'uamsecret'
	option defidletimeout '60'
	option ssid 'MeOS AP'
#	option uamserver 'http://192.168.0.2/dashboard/hotspot/login.html'
	option macauth '0'
	option ipup '/etc/chilli/up.sh'
	option ipdown '/etc/chilli/down.sh'
